{
  "name": "logs.moe",
  "version": "1.0.0",
  "private": true,
  "description": "Share secure logs",
  "author": "hi@fabricio.dev",
  "license": "WTFPL",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=23.5.0"
  },
  "packageManager": "npm@11.0.0",
  "scripts": {
    "start": "node --env-file=.env --watch-path=./static --watch-path=./src/views --watch server.js",
    "build-static": "esbuild ./static/*.mjs ./static/*.css --minify --outdir=./static/dist --out-extension:.js=.mjs"
  },
  "dependencies": {
    "@koa/router": "13.0.1",
    "koa": "2.15.3"
  },
  "devDependencies": {
    "esbuild": "0.24.2"
  }
}
