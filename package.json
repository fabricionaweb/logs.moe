{
  "name": "logs.moe",
  "version": "1.0.0",
  "private": true,
  "description": "Share secure logs from the command line",
  "author": "hi@fabricio.dev",
  "license": "WTFPL",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=23.5.0"
  },
  "packageManager": "npm@11.0.0",
  "scripts": {
    "start": "node --env-file=.env --watch-path=./static --watch-path=./src --watch-path=./server.js server.js",
    "build-static": "esbuild ./static/*.mjs ./static/*.css --minify --outdir=./static/dist --out-extension:.js=.mjs"
  },
  "dependencies": {
    "@koa/bodyparser": "5.1.1",
    "@koa/ejs": "5.1.0",
    "@koa/router": "13.1.0",
    "koa": "2.15.3",
    "koa-static": "5.0.0",
    "nanoid": "5.0.9"
  },
  "devDependencies": {
    "esbuild": "0.24.2"
  }
}
